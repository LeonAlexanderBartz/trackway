{
  "name": "trackway",
  "version": "1.0.0",
  "authors": [
    "Mewes Kochheim <kochheim@develop4edu.de>"
  ],
  "main": "public/index.php",
  "ignore": [
    "**/.*",
    "node_modules",
    "bower_components"
  ],
  "dependencies": {
    "jquery": "~2.1.4",
    "bootstrap": "~3.3.5",
    "font-awesome": "~4.4.0",
    "admin-lte": "~2.3.0",
    "uri.js": "~1.16.0",
    "keymaster": "~1.6.3"
  },
  "overrides": {
    "bootstrap": {
      "main": [
        "fonts/*.*",
        "js/*.js",
        "less/**/*.less"
      ]
    },
    "font-awesome": {
      "main": [
        "fonts/*.*",
        "less/**/*.less"
      ]
    },
    "admin-lte": {
      "main": [
        "dist/img/*.*",
        "dist/js/*.js",
        "build/less/**/*.less",
        "plugins/**/*"
      ]
    }
  }
}
